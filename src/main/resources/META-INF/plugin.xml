<idea-plugin>
    <id>com.tao.getx</id>
    <!-- 插件名称，官方插件库显示的名称 -->
    <name>GetX</name>
    <!-- 版本号  -->
<!--    <version>1.1</version>-->
    <!-- 供应商主页和email（联系地址）-->
    <vendor email="xdd666@foxmail.com" url="https://github.com/CNAD666/getx_template">CNAD666</vendor>

    <!-- 插件的描述 （不能使用默认值，必须修改成自己的。并且需要大于40个字符）-->
    <description><![CDATA[
    <h2>getx_template</h2>
    <ul>
        <li>Used to generate the template code of GetX framework</li>
        <li>GitHub：https://github.com/CNAD666/getx_template</li>
    </ul>
    <h2>User Guide</h2>
    <ul>
        <li>Right-click the File: New -> GetX</li>
    </ul>
    ]]></description>


    <!-- 插件版本变更信息 支持HTML标签-->
<!--    <change-notes><![CDATA[-->
<!--    <h1>1.1</h1>-->
<!--    <ul>-->
<!--        <li>You can generate a large number of GetX template codes</li>-->
<!--        <li>Improve development efficiency</li>-->
<!--        <li>If you have any questions, please give feedback</li>-->
<!--    </ul>-->
<!--    ]]></change-notes>-->

    <!-- 插件所依赖的其他插件的id 建议写上，不写，在在某些情况下，上传插件会报错-->
    <depends>com.intellij.modules.all</depends>

    <!-- 插件兼容IDEA Build 号-->
    <idea-version since-build="173.0" until-build="201.8743.12"/>

    <!-- 编写插件动作 -->
    <actions>
        <!-- Add your actions here -->
        <action id="com.tao.NewGetX" class="NewGetX" text="GetX" description="create GetX template code"
                icon="/image/icon.jpg">
            <add-to-group group-id="NewGroup" anchor="first"/>
        </action>
    </actions>
</idea-plugin>